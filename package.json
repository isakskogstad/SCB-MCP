{
  "name": "scb-mcp",
  "version": "2.4.1",
  "description": "Model Context Protocol server for Statistics Sweden (SCB) PX API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "preinstall": "node scripts/enforce-render-docker.js",
    "build": "tsc",
    "start": "node dist/http-server.js",
    "start:streamable": "node dist/http-server.js",
    "start:stdio": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "npm run test-api",
    "test-api": "npm run build && node test-api.mjs",
    "test-validation": "npm run build && node tests/api-validation.test.mjs",
    "test-local": "npm run build && node local-test.js",
    "test-simple": "npm run build && node simple-test.js",
    "test-full": "npm run build && node test-tools.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "statistics",
    "sweden",
    "scb",
    "px-api",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "marked": "^17.0.1",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
